{
  "name": "lucid-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env HOSTNAME=0.0.0.0 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "node scripts/prisma-generate.js",
    "prisma:generate:force": "node scripts/prisma-generate.js --force",
    "test": "npm run prisma:generate && vitest",
    "test:ui": "npm run prisma:generate && vitest --ui",
    "test:run": "npm run prisma:generate && vitest run",
    "test:coverage": "npm run prisma:generate && vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:e2e:ci": "playwright test --config=playwright.ci.config.ts",
    "test:all:ci": "npm run test:run && npm run test:e2e:ci",
    "playwright:install": "playwright install --with-deps",
    "cleanup:audit-logs": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/cleanup-corrupted-audit-logs.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.70.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.0.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "^15.1.3",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "prisma": "^6.19.1",
    "supabase": "^2.70.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
